@model ContatoModel
@{
    ViewData["title"] = "Excluir contato";
}

-<div class="text-center">
    <h3>Deseja apagar o contato @Model.Nome ?</h3>
</div>

<input type="hidden" asp-for="Id" />
<a class="btn btn-primary" role="button" asp-controller="Contato" asp-action="Index">Cancelar</a>
<a class="btn btn-danger" role="button" asp-route-id="@Model.Id" asp-controller="Contato" asp-action="Deletar">Confirmar</a>